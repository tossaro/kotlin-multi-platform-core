<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="tossaro.android.core.example.app.stocklist.StockListFragment"
        android:label="HomeFragment">
        <deepLink app:uri="android-app://app.tossaro.dev/stocks" />
    </fragment>
    <dialog
        android:id="@+id/stockDetailSheetFgragment"
        android:name="tossaro.android.core.example.app.stockdetailsheet.StockDetailBaseSheetFragment"
        android:label="StockDetailSheetFragment">
        <deepLink app:uri="android-app://app.tossaro.dev/stock/{coin}/{value}" />
    </dialog>
    <fragment
        android:id="@+id/stockEditFgragment"
        android:name="tossaro.android.core.example.app.stockedit.StockEditFragment"
        android:label="StockEditFragment">
        <deepLink app:uri="android-app://app.tossaro.dev/stock/{coin}" />
    </fragment>
</navigation>