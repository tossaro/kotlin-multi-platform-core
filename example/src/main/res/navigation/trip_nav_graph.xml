<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/trip"
    app:startDestination="@id/tripFragment">
    <fragment
        android:id="@+id/tripFragment"
        android:name="tossaro.android.core.example.app.trip.TripFragment"
        android:label="TripFragment">
        <deepLink app:uri="android-app://app.tossaro.dev/trip" />
    </fragment>
    <dialog
        android:id="@+id/dateSheetFragment"
        android:name="tossaro.android.core.app.datesheet.DateSheetFragment"
        android:label="DateSheetFragment">
        <deepLink app:uri="android-app://app.tossaro.dev/select-date/{title}/{selected}/{min}/{max}" />
    </dialog>
</navigation>